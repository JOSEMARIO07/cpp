#include <iostream>

using namespace std;

int main(int argc, char const *argv[])
{
    bool valorBoolean;
    valorBoolean = true;

    int valorEntero = 15;
    double valorDouble = 20.99;
    string valorString = "Hola como estan";
    char valorChar = "B";

    cout << "valor Boolean: " << valor Boolean << endl; 
    cout << "valor Entero: " << valor Entero << endl; 
    cout << "valor Double: " << valor Double << endl; 
    cout << "valor String: " << valor String << endl; 
    cout << "valor Char: " << valor Char << endl; 
    return 0;
}
